{
    "name": "sanmtos/chat",
    "description": "Sanmtos Chat API package for  laravel developers",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanmtos",
            "email": "sanmtos@gmail.com"
        },
        {
            "name": "Philip James Ajagabos",
            "email": "ajagabos007@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^11.1",
        "symfony/var-dumper": "^7.0",
        "phpstan/phpstan": "*"
    },
    "autoload": {
        "psr-4": {
            "Sanmtos\\Chat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sanmtos\\Chat\\Tests\\": "tests/",
            "Sanmtos\\Chat\\Database\\Factories\\": "database/factories/",
            "Sanmtos\\Chat\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sanmtos\\Chat\\ChatServiceProvider"
            ]
        }
    },
   
    "minimum-stability": "stable"
}
